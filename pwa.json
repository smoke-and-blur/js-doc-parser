{
  "name": "Doc Parser",
  "start_url": "/",
  "display": "standalone",
  "descirption": "The app to parse doc files (internal use only)",
  "icons": [
    {
      "src": "parser-icon-v1.svg",
      "sizes": "any"
    }
  ],
  "share_target": {
    "action": "/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "name",
      "text": "description",
      "url": "link",
      "files": [
        {
          "name": "in",
          "accept": [
            "application/vnd.openxmlformats-officedocument.wordprocessingml.document",
            "application/msword"
          ]
        }
      ]
    }
  },
  "theme_color": "#000000",
  "background_color": "#000000",
  "short_name": "Doc Parser",
  "id": "js-doc-parser"
}