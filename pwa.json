{
  "name": "Doc Parser",
  "start_url": "/",
  "display": "standalone",
  "description": "The app to parse doc files (internal use only)",
  "screenshots": [
    {
      "src": "screenshots/screenshot-1.png",
      "sizes": "534x712",
      "type": "image/png"
    }
  ],
  "icons": [
    {
      "src": "icons/any.png",
      "sizes": "1024x1024",
      "purpose": "any"
    },
    {
      "src": "icons/maskable.png",
      "sizes": "1024x1024",
      "purpose": "maskable"
    },
    {
      "src": "icons/monochrome.png",
      "sizes": "1024x1024",
      "purpose": "monochrome"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "in",
          "accept": [
            "application/vnd.openxmlformats-officedocument.wordprocessingml.document",
            "application/msword"
          ]
        }
      ]
    }
  },
  "theme_color": "#000000",
  "background_color": "#000000",
  "short_name": "Doc Parser",
  "id": "js-doc-parser"
}